{
  "name": "@bx-looop/soil-domain-service-runtime",
  "version": "1.1.0",
  "description": "Soil domain service",
  "scripts": {
    "build": "tsc && cp ../.npmignore dist/ && cp package.json dist/",
    "test": "npx jest",
    "postversion": "cp -r package.json ..",
    "local:publish-config": "sed -i '' -e 's/s:\\/\\/npm.pkg.github.com/:\\/\\/localhost:4873/g' dist/package.json",
    "local:publish": "npm run build && npm run local:publish-config && cd dist && npm unpublish --force && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bx-LOOOP/soil-domain-service.git"
  },
  "author": "Bx",
  "license": "ISC",
  "bugs": {
    "url": "https://bx-looop.atlassian.net/secure/RapidBoard.jspa?projectKey=LOOOP"
  },
  "homepage": "https://github.com/Bx-LOOOP/soil-domain-service#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.75",
    "@types/uuid": "^8.3.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "ajv": "^8.2.0",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.886.0",
    "aws-sdk-mock": "^5.1.0",
    "csvtojson": "^2.0.10",
    "uuid": "^8.3.2",
    "xlsx": "^0.16.9"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
