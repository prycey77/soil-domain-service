{
  "name": "@bx-looop/soil-domain-service",
  "version": "1.1.0",
  "description": "A template for LOOOP service repositories",
  "scripts": {
    "build": "tsc && cp ../.npmignore dist/ && cp package.json dist/",
    "postversion": "cp -r package.json .. && node ../../bin/post-version.js",
    "local:install": "rm -rf node_modules && rm package-lock.json && rm -rf /tmp/empty-cache && npm install --cache /tmp/empty-cache",
    "local:publish-config": "sed -i '' -e 's/s:\\/\\/npm.pkg.github.com/:\\/\\/localhost:4873/g' dist/package.json",
    "local:publish": "npm run build && npm run local:publish-config && cd dist && node ../../bin/post-version.js && npm unpublish --force && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bx-LOOOP/soil-domain-service.git"
  },
  "author": "Bx",
  "license": "ISC",
  "bugs": {
    "url": "https://bx-looop.atlassian.net/secure/RapidBoard.jspa?projectKey=LOOOP"
  },
  "homepage": "https://github.com/Bx-LOOOP/soil-domain-service#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "dependencies": {
    "@bx-looop/soil-domain-service-runtime": "file://../src/dist",
    "@pulumi/aws": "^3.38.1",
    "@pulumi/awsx": "^0.24.0",
    "@pulumi/pulumi": "^2.25.2"
  },
  "devDependencies": {
    "typescript": "^4.2.4"
  },
  "pulumi": {
    "runtimeDependencies": {
      "@bx-looop/soil-domain-service-runtime": "file://../src/dist"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
